<?xml version="1.0" encoding="UTF-8"?>
<itop_design
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0">
	<constants>
	</constants>
	<classes>
		<class id="PhysicalDevice" _delta="must_exist">
			<fields>
				<field id="status" xsi:type="AttributeEnum" _delta="redefine">
					<values>
						<value>production</value>
						<value>implementation</value>
						<value>stock</value>
						<value>obsolete</value>
						<value>decommission</value>
						<value>reserve</value>
					</values>
					<sql>status</sql>
					<default_value>stock</default_value>
					<is_null_allowed>true</is_null_allowed>
					<display_style>list</display_style>
				</field>
				<field id="maintenance_date" xsi:type="AttributeDate"
					_delta="define">
					<sql>maintenance_date</sql>
					<default_value />
					<is_null_allowed>true</is_null_allowed>
				</field>
				<field id="maintenance_frequency" xsi:type="AttributeInteger"
					_delta="define">
					<sql>maintenance_frequency</sql>
					<default_value>0</default_value>
					<is_null_allowed>true</is_null_allowed>
				</field>
			</fields>
			<presentation>
				<details _delta="merge">
					<items>
						<item id="maintenance_date" _delta="define">
							<rank>125</rank>
						</item>
						<item id="maintenance_frequency" _delta="define">
							<rank>126</rank>
						</item>
					</items>
				</details>
				<search _delta="merge">
					<items>
						<item id="maintenance_date" _delta="define">
							<rank>40</rank>
						</item>
						<item id="maintenance_frequency" _delta="define">
							<rank>41</rank>
						</item>
					</items>
				</search>
				<list _delta="merge">
					<items>
						<item id="maintenance_date" _delta="define">
							<rank>125</rank>
						</item>
						<item id="maintenance_frequency" _delta="define">
							<rank>126</rank>
						</item>
					</items>
				</list>
			</presentation>
		</class>
	</classes>
	<user_rights>
	</user_rights>
</itop_design>
